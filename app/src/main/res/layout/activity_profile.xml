<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Profile">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/profile_test" />


        <TextView
            android:id="@+id/dynamic_user"
            android:layout_width="267dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="54dp"
            android:layout_marginTop="423dp"
            android:background="@null"
            android:fontFamily="@font/rubik_medium"
            android:inputType="textPersonName"
            android:textColor="#2F3E46"
            android:textSize="30sp" />

        <TextView
            android:id="@+id/dynamic_email"
            android:layout_width="313dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="55dp"
            android:layout_marginTop="513dp"
            android:background="@null"
            android:fontFamily="@font/rubik_medium"
            android:inputType="textPersonName"
            android:textColor="#2F3E46"
            android:textSize="25sp" />

        <TextView
            android:id="@+id/dynamic_bal"
            android:layout_width="267dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="55dp"
            android:layout_marginTop="598dp"
            android:background="@null"
            android:fontFamily="@font/rubik_medium"
            android:inputType="textPersonName"
            android:textColor="#2F3E46"
            android:textSize="30sp" />

        <Button
            android:id="@+id/nav_dash"
            android:layout_width="58dp"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="306dp"
            android:layout_marginBottom="16dp"
            android:onClick="nav_dash"
            android:alpha="0.0" />

        <Button
            android:id="@+id/nav_expense"
            android:layout_width="58dp"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="244dp"
            android:layout_marginBottom="17dp"
            android:onClick="nav_expense"
            android:alpha="0.0" />

        <Button
            android:id="@+id/nav_home"
            android:layout_width="58dp"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="176dp"
            android:layout_marginBottom="17dp"
            android:onClick="nav_home"
            android:alpha="0.0" />

        <Button
            android:id="@+id/nav_income"
            android:layout_width="58dp"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="108dp"
            android:layout_marginBottom="18dp"
            android:onClick="nav_income"
            android:alpha="0.0" />

        <Button
            android:id="@+id/view_account"
            android:layout_width="127dp"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="225dp"
            android:layout_marginBottom="98dp"
            android:onClick="view_acc"
            android:alpha="0.0" />

        <Button
            android:id="@+id/add_account"
            android:layout_width="127dp"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="66dp"
            android:layout_marginBottom="98dp"
            android:alpha="0.0" />

    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>